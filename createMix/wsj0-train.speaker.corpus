    <segment end="inf" name="01mc0218_0000000000_inf" start="0.0">
      <speaker name="01m"/>
      <orth> That makes the problem a little harder </orth>
    </segment>
  </recording>
  <recording audio="/u/corpora/speech/wsj/audio/01m/01mc0219.sph" name="01mc0219">
    <segment end="inf" name="01mc0219_0000000000_inf" start="0.0">
      <speaker name="01m"/>
      <orth> Let me think for a minute </orth>
    </segment>
  </recording>
  <recording audio="/u/corpora/speech/wsj/audio/01m/01mc021a.sph" name="01mc021a">
    <segment end="inf" name="01mc021a_0000000000_inf" start="0.0">
