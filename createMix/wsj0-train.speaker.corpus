  <recording audio="/u/corpora/speech/wsj/audio/40i/40ic020x.sph" name="40ic020x">
    <segment end="inf" name="40ic020x_0000000000_inf" start="0.0">
      <speaker name="40i"/>
      <orth>
        In an unusual step it received approval of the plan from stock and
