    </segment>
  </recording>
  <recording audio="/u/corpora/speech/wsj/audio/01l/01lo031e.sph" name="01lo031e">
    <segment end="inf" name="01lo031e_0000000000_inf" start="0.0">
      <speaker name="01l"/>
      <orth>
