  </recording>
  <recording audio="/u/corpora/speech/wsj/audio/01m/01mc020m.sph" name="01mc020m">
    <segment end="inf" name="01mc020m_0000000000_inf" start="0.0">
      <speaker name="01m"/>
      <orth>
