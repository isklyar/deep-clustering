    </segment>
  </recording>
  <recording audio="/u/corpora/speech/wsj/audio/40d/40dc020e.sph" name="40dc020e">
    <segment end="inf" name="40dc020e_0000000000_inf" start="0.0">
      <speaker name="40d"/>
      <orth>
