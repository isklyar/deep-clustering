  </recording>
  <recording audio="/u/corpora/speech/wsj/audio/01m/01mc020a.sph" name="01mc020a">
    <segment end="inf" name="01mc020a_0000000000_inf" start="0.0">
      <speaker name="01m"/>
      <orth> With the Americans we don't know where we stand </orth>
    </segment>
