      </orth>
    </segment>
  </recording>
  <recording audio="/u/corpora/speech/wsj/audio/01m/01mc020f.sph" name="01mc020f">
    <segment end="inf" name="01mc020f_0000000000_inf" start="0.0">
      <speaker name="01m"/>
      <orth>
