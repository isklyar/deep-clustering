      </orth>
    </segment>
  </recording>
  <recording audio="/u/corpora/speech/wsj/audio/01u/01uc020l.sph" name="01uc020l">
    <segment end="inf" name="01uc020l_0000000000_inf" start="0.0">
      <speaker name="01u"/>
      <orth>
