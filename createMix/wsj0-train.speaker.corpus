  <recording audio="/u/corpora/speech/wsj/audio/01m/01mc021b.sph" name="01mc021b">
    <segment end="inf" name="01mc021b_0000000000_inf" start="0.0">
      <speaker name="01m"/>
      <orth>
        <noise>
